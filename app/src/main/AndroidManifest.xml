<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookkarovendor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".shopservicesprovider.ShopServicesActivity"
            android:label="@string/title_activity_shop_services">
        </activity>
        <activity
            android:name=".deliveryservicesprovider.DeliveryServicesActivity"
            android:label="@string/title_activity_delivery_services">
        </activity>
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".householdserviceprovider.HouseholdServicesActivity"
            android:label="@string/app_name" />
    </application>

</manifest>